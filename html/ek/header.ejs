  display: block;
}

.wrapper .sidebar ul li a .fas{
text-align: center;
  width: 35px;
  transition: 0.7s;
}

.wrapper .sidebar ul li:hover{
  text-align: center;
  background-color: #cdceff;
  transition: 0.7s;
}
    
.wrapper .sidebar ul li:hover a{
  color: #fff;
  transition: 0.7s;
}

.wrapper .main_content{
  width: 100%;
  margin-left: 200px;
}

.wrapper .main_content .header{
  padding: 20px;
  background: #90CAF9;
