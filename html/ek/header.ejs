  height: 150px;
  box-shadow: 0px 0px 10px #FFA726;
  transition:all 0.5s;
}

.wrapper .main_content .info img:hover{
  margin-right: 20px;
  margin: 5px;
  color: Black;
  line-height: 35px;
  border-radius: 10%;
  width: 170px;
  height: 170px;
  box-shadow: 5px 5px 10px #FFA726;
  transition:all 0.5s;
}

.wrapper .main_content .info div{
  margin-bottom: 40px;
}

