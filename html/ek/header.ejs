}

*{
  margin: 0;
  padding: 0;
  box-sizing: border-box;
  list-style: none;
  text-decoration: none;
  font-family: 'Josefin Sans', sans-serif;
}

body{
   background-color: #1c1c1c;
}

.wrapper{
  display: flex;
  position: relative;
}

.wrapper .sidebar{
  width: 200px;
  height: 100%;
  background: #5260ff;
  padding: 30px 0px;
  position: fixed;
}

.wrapper .sidebar .img img{
  color: #fff;
  text-transform: uppercase;
  text-align: center;
  margin-bottom: 25px;
  padding: 15px;
  font-size: 20px;
}

.wrapper .sidebar img{
  margin-left: 18%;
}

.wrapper .sidebar ul li{
  padding: 15px;
  transition: 0.7s;
}    

.wrapper .sidebar ul li a{
